{
    "name": "chris1278/cookie",
    "type": "phpbb-extension",
    "description": "Loads scripts with which you give the users of the forum page the opportunity to reject or accept cookies!",
    "homepage": "https://christian-esch.de",
    "version": "2.0.3",
    "time": "2020-12-28",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Chris1278",
            "email": "smilie-man@gmx.de",
            "homepage": "https://christian-esch.de",
            "role": "Lead Developer"
        },
        {
            "name": "Kirk",
            "email": "canam800er@googlemail.com",
            "homepage": "https://reyno41.bplaced.net/phpbb",
            "role": "Developer"
        },
        {
            "name": "69bruno",
            "email": "stephan-kaufmann@arcor.de",
            "homepage": "https://cruiser-lounge.de/",
            "role": "Contributor"
        }
    ],
    "require": {
        "php": ">=7.0.0,<9.0.0",
        "phpbb/phpbb": ">=3.2.10,<3.4.0@dev",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Opt-In Cookie Manager",
        "lang-min-ver": "2.0.0",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.10,<3.4.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/opt_in_cookie_manager",
            "filename": "version_check",
            "ssl": true
        }
    }
}
